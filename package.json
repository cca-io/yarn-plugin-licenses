{
  "name": "@yarnpkg/plugin-licenses",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "main": "./sources/index.ts",
  "license": "UNLICENSED",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "build": "yarn builder build plugin",
    "lint": "biome check sources tests",
    "format": "biome format --write sources tests",
    "typecheck": "tsc --noEmit",
    "test:unit": "yarn compile && node --test dist/tests/unit/**/*.test.js",
    "test:integration": "yarn build && yarn compile && node --test dist/tests/integration/**/*.test.js",
    "test": "yarn lint && yarn typecheck && yarn test:unit && yarn test:integration"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@types/node": "latest",
    "@yarnpkg/builder": "latest",
    "@yarnpkg/cli": "latest",
    "@yarnpkg/core": "latest",
    "@yarnpkg/fslib": "latest",
    "clipanion": "latest",
    "typescript": "latest"
  }
}
